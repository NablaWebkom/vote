    "angular": "1.5.5",
    "angular-local-storage": "0.2.7",
    "angular-route": "1.5.5",
    "angular-ui-bootstrap": "1.3.3",
    "bcrypt": "1.0.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "bootstrap": "~3.3.6",
    "chalk": "2.3.0",
    "commander": "2.13.0",
    "connect-flash": "0.1.1",
    "connect-mongo": "2.0.1",
    "cookie-parser": "1.4.3",
    "css-loader": "0.26.1",
    "csurf": "1.9.0",
    "express": "4.16.2",
    "express-promise-router": "3.0.1",
    "express-session": "1.15.6",
    "extract-text-webpack-plugin": "1.0.1",
    "font-awesome-webpack": "0.0.4",
    "lodash": "4.17.4",
    "method-override": "2.3.10",
    "mongoose": "5.0.1",
    "nib": "1.1.2",
    "nyc": "11.4.1",
